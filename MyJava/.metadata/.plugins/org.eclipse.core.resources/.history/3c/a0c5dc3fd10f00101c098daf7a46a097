public class ListHashSet {
	public static void main(String[] args) {
		List<Integer> list = new ArrayList<>();
		try {
			Scanner sc = new Scanner(System.in);
			int size = sc.nextInt();
			if(size<10 || size>20) {
				throw new Exception();
			}
		} catch(Exception e) {
			System.out.println("10 ~ 20 사이의 값으로 다시 입력하세요.");
			return;
		}
		
