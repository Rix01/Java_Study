package com.sec02;
	public class BitOperator {
		
		public static void prn() {
			int a=51;
			int b=240;
			
			 System.out.println("AND ="+( a&b));
			 System.out.println("OR=" +(a|b));
			 System.out.println("XOR="+(a^b));
		}
		
		public static void main(String[] args) {
			prn();
		}
	}




	
	
